QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=planes_gobierno

# Google AI
GOOGLE_API_KEY=your_google_api_key_here

# Embedding Configuration
# Options: sentence_transformers, openai
EMBEDDING_PROVIDER=sentence_transformers
# For sentence_transformers: sentence-transformers/all-MiniLM-L6-v2
# For OpenAI: text-embedding-3-small, text-embedding-3-large, text-embedding-ada-002
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
# Required only if using OpenAI embeddings
OPENAI_API_KEY=

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=20
